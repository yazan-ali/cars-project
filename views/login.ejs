<%- include("./partials/header") %>
<div class=" loginForm">
	<h1>Log in</h1>
<div class="row">
	<div class=" col-md-6">
		<form  action="/login" method="POST">

<div class="form-group">
    <label for="exampleInputEmail1">Email address</label>
    <input type="email" class="form-control" id="exampleInputEmail1" name="username" placeholder="email" aria-describedby="emailHelp" required>
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Password</label>
    <input type="password" class="form-control" name="password" placeholder="password" id="exampleInputPassword1" required>
  </div>
	<button class="btn btn-secondary">Log in</button>
</form>
	</div>
</div>

</div>



